{
  "name": "reps",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "node ./src/script/auto-increment.js && node ./src/script/generate-easignore.js && eas build",
    "build:android": "npm run build -- --platform android",
    "build-test:android": "npm run build -- --platform android --profile preview",
    "build:web": "npx expo export -p web",
    "deploy:android": "eas submit --platform android",
    "deploy-test:android": "eas submit --platform android --preview",
    "test": "jest --coverage",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build-deploy-test:android": "npm run build-test:android && npm run deploy-test:android",
    "build-deploy:android": "npm run build:android && npm run deploy:android",
    "build-deploy:web": "npm run build:web && firebase deploy",
    "eject": "expo eject",
    "pretty": "npx prettier"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-masked-view/masked-view": "0.2.9",
    "@react-navigation/bottom-tabs": "^6.0.0",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.0",
    "expo": "^49.0.0",
    "expo-dev-client": "~2.4.12",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "firebase": "^10.4.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-markdown-display-updated": "^7.0.0",
    "react-native-paper": "^4.12.6",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^7.2.6",
    "typescript": "^5.1.3",
    "whatwg-fetch": "^3.6.17",
    "expo-linking": "~5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "eslint-config-custom": "*"
  },
  "private": true,
  "version": "1.0.44"
}
